  },
  "homepage": "https://github.com/yormi/react-lazy-infinite#readme",
  "peerDependencies": {
    "react": "15.x.x",
    "react-addons-pure-render-mixin": "15.x.x",
    "react-addons-test-utils": "15.x.x",
